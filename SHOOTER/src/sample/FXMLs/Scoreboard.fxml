      </TableView>
      <RadioButton fx:id="SilverTick" layoutX="492.0" layoutY="105.0" mnemonicParsing="false" text="Silver" textFill="#f3f5f5" />
      <RadioButton fx:id="GoldTick" layoutX="492.0" layoutY="134.0" mnemonicParsing="false" text="Gold" textFill="#f8f6f6" />
